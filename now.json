{
	"version": 2,
	"routes": [
		{
			"src": "/",
			"dest": "/api/index"
		},
		{
			"src": "/.+",
			"headers": { "Access-Control-Allow-Origin": "*" },
			"continue": true
		},
		{
			"src": "/([^/]+)/([^/]+)/([^/]*)(/.*)?",
			"dest": "/api/get-path?owner=$1&repo=$2&ref=$3&path=$4"
		},
		{
			"src": "/([^/]+)/?(.*)",
			"dest": "/api/get-repo?owner=$1&repo=$2"
		}
	]
}